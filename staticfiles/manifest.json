{
    "manifest_version": 3,
    "name": "Alinma Bank",
    "version": "1.0",
    "author": "Ibrahim Shaher Saeed",
    "dir": "auto",
    "lang": "ar",
    "display": "standalone",
    "display_override": [
        "fullscreen",
        "minimal-ui"
    ],
    "orientation": "any",
    "start_url": "/?homescreen=1",
    "id": "/?homescreen=1",
    "background_color": "#fff",
    "theme_color": "#fff",
    "action": {
        "default_popup": "/",
        "default_title": "بنك الانماء الرئيسية"
    },
    "description": "المساهمة في التنمية الاقتصادية والاجتماعية وفي تحسين مستوى معيشة المجتمع اليمني عن طريق تقديم خدمات مالية متنوعة ومتميز",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "icons": [
        {
            "src": "assets/icon/android-icon-36x36.png",
            "sizes": "36x36",
            "type": "image/png",
            "density": "0.75"
        },
        {
            "src": "assets/icon/android-icon-48x48.png",
            "sizes": "48x48",
            "type": "image/png",
            "density": "1.0"
        },
        {
            "src": "assets/icon/android-icon-72x72.png",
            "sizes": "72x72",
            "type": "image/png",
            "density": "1.5"
        },
        {
            "src": "assets/icon/android-icon-96x96.png",
            "sizes": "96x96",
            "type": "image/png",
            "density": "2.0"
        },
        {
            "src": "assets/icon/android-icon-144x144.png",
            "sizes": "144x144",
            "type": "image/png",
            "density": "3.0"
        },
        {
            "src": "assets/icon/android-icon-192x192.png",
            "sizes": "192x192",
            "type": "image/png",
            "density": "4.0"
        },
        {
            "src": "assets/icon/android-chrome-192x192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "assets/icon/android-chrome-512x512.png",
            "sizes": "512x512",
            "type": "image/png"
        }
    ]
}